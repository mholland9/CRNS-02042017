                mediator.pub(pageInstance.eventName);
                var rid = pageInstance.getActivityId();
                url = url.replace(/([?&]rid=)[^&#]*/i, "$1" + rid);
            }
            new Image().src = url.replace(/&amp;/g, '&');
        });
